<h2 class="flex justify-center text-2xl font-bold">Filter items:</h2>
<div class="flex flex-col mt-10 gap-5">
    <p class="flex justify-center text-xl">Brand:</p>
    <div class="flex flex-col gap-2">
        <% carBrandsAll.forEach(brand => { %>
        <label>
            <input onclick="submitForm()" type="checkbox" name="checkboxBrand" value="<%= brand.name %>"
                <%= (checkboxesChecked && checkboxesChecked.includes(brand.name)) ? 'checked' : '' %>>
            <%= brand.name  %>
        </label>
        <% }) %>
    </div>
</div>
<div class="flex flex-col gap-2 justify-center mt-10 p-3">
    <p>Range of price:</p>
    <div class="relative w-52">
        <span class="top-0 left-0"><%= minPrice %></span>
        <input type="range" min="0" max="100" value="50" class="w-52">
        <span class="absolute top-0 right-0"><%= maxPrice %></span>
    </div>

</div>